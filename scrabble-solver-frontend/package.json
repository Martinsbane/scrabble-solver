{
  "name": "scrabble-solver-frontend",
  "version": "1.0.0",
  "description": "Scrabble Solver - Frontend",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:prod": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "API_HOST": "http://kamilmielnik.pl",
        "API_PORT": "5000"
      }
    },
    "start": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "http-server dist",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/scrabble-solver.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "https://kamilmielnik.github.io/"
  },
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/scrabble-solver/issues"
  },
  "homepage": "https://github.com/kamilmielnik/scrabble-solver#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "re-reselect": "^1.0.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.4.0",
    "react-intl-redux": "^0.6.0",
    "react-redux": "^5.0.6",
    "react-virtualized": "^9.9.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "scrabble-solver-commons": "file:../scrabble-solver-commons"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "better-npm-run": "0.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-modules": "^0.8.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "style-ext-html-webpack-plugin": "^3.4.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
